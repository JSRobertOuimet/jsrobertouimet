    .overlay-container
      a(href="project-mprs.html").link.no-underline.primary-color Master Piano Recital Series
      a(href="project-la-parapharmacie").link.no-underline.primary-color La Parapharmacie
      a(href="project-mimib").link.no-underline.primary-color mimiB
      a(href="project-assent-compliance").link.no-underline.primary-color Assent Compliance
      a(href="project-concerts-by-the-canal").link.no-underline.primary-color Concerts by the Canal
