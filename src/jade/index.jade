    //-   a(href="http://www.french.html").no-underline FR
  
  main
    .main-container
      header
        h1 
          | Jean-Simon
          br
          | Robert Ouimet
          sup
            i(class="fa fa-question-circle" aria-hidden="true")
        p Designer &amp; Developer

      footer
          a(href="mailto:hello@jsrobertouimet.com").link hello@jsrobertouimet.com
          p
            a(href="https://www.behance.net/JSRobertOuimet" target="_blank").social-link
              i(class="fa fa-behance" aria-hidden="true")
            a(href="https://github.com/JSRobertOuimet" target="_blank").social-link
              i(class="fa fa-github" aria-hidden="true")
            a(href="https://ca.linkedin.com/in/jsrobertouimet" target="_blank").social-link
