doctype html
html(lang="en")
head
  meta(charset="UTF-8")
  meta(name="viewport", content="width=device-width, initial-scale=1")
  
  link(href="css/main.min.css", rel="stylesheet")
  
  script(src="https://use.typekit.net/crq2gzs.js")
  script try{Typekit.load({async: true});}catch(e){}
  script(src="https://use.fontawesome.com/82563c2d95.js")
  
  title Jean-Simon Robert Ouimet

body
  nav.main-nav
    a(href="#")
      i(class="fa fa-bars menu-icon" aria-hidden="true" onclick="openNav()")
    
  #nav.overlay
